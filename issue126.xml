<?xml version="1.0" encoding="utf-8"?>
<Configuration>
    <Author>
        <Name>S. V. Paulauskas</Name>
        <Email>stanpaulauskas@gmail.com</Email>
        <Date>February 28, 2017</Date>
    </Author>

    <Description>
        A timing setup to measure time difference between two pulser signals
    </Description>

    <Global>
        <Revision version="F"/>
        <BitResolution value="12"/>
        <EventWidth unit="s" value="1e-6"/>
        <HasRaw value="false"/>
    </Global>

    <DetectorDriver>
        <Analyzer name="WaveformAnalyzer"/>
        <Analyzer name="FittingAnalyzer" type="gsl"/>
        <Processor name="TwoChanTimingProcessor"/>
    </DetectorDriver>

    <Map verbose_calibration="False" verbose_map="False" verbose_walk="False">
        <Module number="0">
            <Channel number="0" type="pulser" subtype="start">
                <Fit>beta="0.4335" gamma="0.03985"</Fit>
                <Trace>RangeLow="8" RangeHigh="20" baselineThreshold="3" delay="200"</Trace>
            </Channel>
            <Channel number="1" type="pulser" subtype="stop">
                <Fit>beta="0.4335" gamma="0.03985"</Fit>
                <Trace>RangeLow="8" RangeHigh="20" baselineThreshold="3" delay="200"</Trace>
            </Channel>
        </Module>
    </Map>

    <!-- Empty since we are not doing any cross event correlations -->
    <TreeCorrelator name="root" verbose="False">
    </TreeCorrelator>
</Configuration>
